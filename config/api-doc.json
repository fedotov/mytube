{
  "swagger": "2.0",
  "info": {
    "version": "0.0.1",
    "title": "Mytube"
  },
  "basePath": "/",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json",
    "text/plain"
  ],
  "paths": {
    "/status": {
      "x-swagger-router-controller": "status",
      "get": {
        "description": "Get current app status",
        "operationId": "status",
        "responses": {
          "200": {
            "description": "Application status",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "UP"
                },
                "status_message": {
                  "type": "string",
                  "example": "ok"
                },
                "app_version": {
                  "type": "string",
                  "example": "0.1.0"
                },
                "app_name": {
                  "type": "string",
                  "example": "tc-mytube-service"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "schema": {
              "$ref": "#/definitions/ErrorResponse"
            }
          }
        }
      }
    },
    "/swagger": {
      "x-swagger-pipe": "swagger_raw"
    }
  },
  "definitions": {
    "ErrorResponse": {
      "required": [
        "message"
      ],
      "properties": {
        "message": {
          "type": "string"
        }
      }
    }
  }
}
